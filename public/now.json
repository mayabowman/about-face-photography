{
  "version": 2,
  "alias": "sam-smith-studio",
  "name": "sam-smith-studio",
  "routes": [
      { "handle": "filesystem" },
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
       },
       {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
       },
      { "src": "/.*",
        "dest": "/index.html" }
  ]
}